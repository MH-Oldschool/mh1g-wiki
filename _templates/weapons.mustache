<div id="damage-calculator">
	<div id="sidebar-content">
		<div class="columns-container">
			<div class="weapon-column">
				<h2>Weapon: <span id="calc-weapon-icon"></span> <span id="calc-weapon-name"></span></h2><br/>

				<h3>Attack: <span id="calc-weapon-attack"></span></h3><br/>
				<h3 id="calc-weapon-element-container" class="blademaster-stat">Element: <img id="calc-weapon-element-icon" class="attribute-icon" src="" alt="" title=""/><span id="calc-weapon-element"></span> (<span id="calc-true-element"></span>)<br/></h3>
				<h3 id="calc-weapon-status-container" class="blademaster-stat">Status: <img id="calc-weapon-status-icon" class="attribute-icon" src="" alt="" title=""/><span id="calc-weapon-status"></span> (<span id="calc-true-status"></span>)<br/></h3>

				<div class="sharpness-range-color blademaster-stat">
					<div id="calc-sharpness-color"></div>
					<div class="calc-sharpness-container">
						<canvas id="calc-sharpness-canvas" class="hide" width="119" height="2"></canvas>
						<div class="sharpness-container"><img id="calc-sharpness-img" src=""/></div>
						<input id="calc-sharpness" type="range" min="0" max step="1"/>
					</div>
				</div>
			</div>

			<div>
				<div class="columns-container">
					<div class="column">
						<h4>Food/Item Skills:</h4>

						<div class="radio-container">
							<input id="calc-food-attack-up-none" type="radio" name="item_attack_up" checked/>
							<div class="radio-icon"></div>
							<label for="calc-food-attack-up-none" title="0">No Attack Up</label>
						</div>
						<div class="radio-container">
							<input id="calc-food-attack-up-small" type="radio" name="item_attack_up"/>
							<div class="radio-icon"></div>
							<label for="calc-food-attack-up-small" title="3">Attack Up Small</label>
						</div>
						<div class="radio-container mhg">
							<input id="calc-food-attack-up-medium" type="radio" name="item_attack_up"/>
							<div class="radio-icon"></div>
							<label for="calc-food-attack-up-medium" title="5">Attack Up Medium</label>
						</div>
						<div class="radio-container">
							<input id="calc-food-attack-up-large" type="radio" name="item_attack_up"/>
							<div class="radio-icon"></div>
							<label for="calc-food-attack-up-large" title="5 (MH1) 10 (MHG)">Attack Up Large</label>
						</div>
						<div class="radio-container mh1">
							<input id="calc-food-attack-up-extra" type="radio" name="item_attack_up"/>
							<div class="radio-icon"></div>
							<label for="calc-food-attack-up-extra" title="10">Attack Up Extra Large</label>
						</div>
						<hr/>
						<div class="checkbox-container">
							<input id="calc-item-attack-up" type="checkbox"/>
							<div class="radio-icon"></div>
							<label for="calc-item-attack-up" title="10">Demon Seed/Flute Buff</label>
						</div>
					</div>

					<div class="column">
						<h4>Armor Skills:</h4>

						<div class="radio-container">
							<input id="calc-attack-up-none" type="radio" name="attack_up" checked/>
							<div class="radio-icon"></div>
							<label for="calc-attack-up-none" title="0">No Attack Up</label>
						</div>
						<div class="radio-container">
							<input id="calc-attack-up-small" type="radio" name="attack_up"/>
							<div class="radio-icon"></div>
							<label for="calc-attack-up-small" title="3">Attack Up Small</label>
						</div>
						<div class="radio-container mhg">
							<input id="calc-attack-up-medium" type="radio" name="attack_up"/>
							<div class="radio-icon"></div>
							<label for="calc-attack-up-medium" title="5">Attack Up Medium</label>
						</div>
						<div class="radio-container">
							<input id="calc-attack-up-large" type="radio" name="attack_up"/>
							<div class="radio-icon"></div>
							<label for="calc-attack-up-large" title="5 (MH1) 10 (MHG)">Attack Up Large</label>
						</div>
						<hr class="blademaster-stat mhg"/>
						<div class="checkbox-container blademaster-stat mhg">
							<input id="calc-handicraft" type="checkbox"/>
							<div class="radio-icon"></div>
							<label for="calc-handicraft">Handicraft</label>
						</div>
						<hr class="mhg"/>
						<div class="checkbox-container mhg">
							<input id="calc-special-attack" type="checkbox"/>
							<div class="radio-icon"></div>
							<label for="calc-special-attack" title="Status x1.125">Special Attack</label>
						</div>
					</div>

					<div class="column">
						<h4 class="gunner-stat mhg">Shot Up Skills:</h4>

						<div class="checkbox-container gunner-stat mhg">
							<input id="calc-normal-shot-up" type="checkbox"/>
							<div class="radio-icon"></div>
							<label for="calc-normal-shot-up">Normal Shot Up</label>
						</div>
						<div class="checkbox-container gunner-stat mhg">
							<input id="calc-pierce-shot-up" type="checkbox"/>
							<div class="radio-icon"></div>
							<label for="calc-pierce-shot-up">Pierce Shot Up</label>
						</div>
						<div class="checkbox-container gunner-stat mhg">
							<input id="calc-pellet-shot-up" type="checkbox"/>
							<div class="radio-icon"></div>
							<label for="calc-pellet-shot-up">Pellet Shot Up</label>
						</div>

						<h4>Items:</h4>

						<div class="checkbox-container">
							<input id="calc-powercharm" type="checkbox"/>
							<div class="radio-icon"></div>
							<label for="calc-powercharm" title="5">Powercharm</label>
						</div>

						<div class="checkbox-container mhg">
							<input id="calc-powertalon" type="checkbox"/>
							<div class="radio-icon"></div>
							<label for="calc-powertalon" title="10">Powertalon</label>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="table-container">
			<table>
				<caption class="blademaster-stat">Motion Values</caption>
				<caption class="gunner-stat">Ammo Damage</caption>

				<thead>
					<tr class="blademaster-stat">
						<th colspan="2">Attack</th>
						<th class="mv-column">Motion Value</th>
						<th>Raw Damage</th>
					</tr>

					<tr class="gunner-stat">
						<th>Shot Name</th>
						<th>Power</th>
						<th># of Impacts</th>
						<th>Effect</th>
					</tr>
				</thead>

				<tbody class="motion-values greatsword-mvs">
					{{#motionValues.greatsword}}
						{{> motion_value_rows }}
					{{/motionValues.greatsword}}
				</tbody>

				<tbody class="motion-values hammer-mvs">
					{{#motionValues.hammer}}
						{{> motion_value_rows }}
					{{/motionValues.hammer}}
				</tbody>

				<tbody class="motion-values lance-mvs">
					{{#motionValues.lance}}
						{{> motion_value_rows }}
					{{/motionValues.lance}}
				</tbody>

				<tbody class="motion-values sword-mvs">
					{{#motionValues.sword}}
						{{> motion_value_rows }}
					{{/motionValues.sword}}
				</tbody>

				<tbody class="motion-values dual-swords-mvs mhg">
					{{#motionValues.dualSwords}}
						{{> motion_value_rows }}
					{{/motionValues.dualSwords}}
				</tbody>

				<tbody class="bowgun-ammo">
					{{#bowgunAmmo}}
						<tr id="{{id}}-row" class="bowgun-shot-row" data-power="{{power}}">
							<td>{{name}}</td>
							<td class="bowgun-shot-power"></td>
							<td>{{{impacts}}}{{^impacts}}1{{/impacts}}</td>
							<td>
								{{#effect}}{{{effect}}} {{/effect}}
								{{#attributeAlt}}
									<img class="attribute-icon" src="images/attributes/{{attribute}}.png" alt="{{attributeAlt}}" title="{{attributeAlt}}"/>
								{{/attributeAlt}}
								<span class="bowgun-shot-attribute" data-attribute="{{attribute}}" data-attribute-value="{{attributeValue}}">
									{{#attributeValue}}{{attributeValue}}{{/attributeValue}}
								</span>
								{{#attributeMod}}
									<span id="ammo-{{attribute}}-attribute"></span>
								{{/attributeMod}}
								{{#explosion}}
									Explosion: {{explosion.power}} <img class="attribute-icon" src="images/attributes/{{explosion.attribute}}.png" alt="{{explosion.attributeAlt}}" title="{{explosion.attributeAlt}}"/>{{explosion.attributeValue}}
									{{#explosion.impacts}} x{{explosion.impacts}} {{/explosion.impacts}}
								{{/explosion}}
							</td>
						</tr>
					{{/bowgunAmmo}}
				</tbody>
			</table>
		</div>
	</div>

	<button id="toggle-damage-calculator">
		<div class="button-copy">Damage Calculator</div>
	</button>
</div>

{{> foot}}
